<!DOCTYPE html>
<html lang="ru">
<head>
	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0 320;0,400;0,700;1 320&display=swap" rel="stylesheet">
	<!-- ViewPort -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- FotnAwesome -->
	<script src="https://kit.fontawesome.com/1299eaddbb.js" crossorigin="anonymous"></script>
	<!-- Bootstrap -->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
	<!-- style.css -->
	<link rel="stylesheet" href="css/style.css">
	<meta charset="UTF-8">
	<title>printer</title>
</head>
<body>
<!-- @import "navbar.kit" -->

<div class="login__bg">
  <section id="login" class="bg-light py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-6 mx-auto">
          <div class="card">
            <div class="card-header text-white login__label">
              <h4>
                <i class="fas fa-sign-in-alt"></i> Login</h4>
            </div>
            <div class="card-body pb-0">
              <form action="index.html">
                <div class="form-group">
                  <label for="username">Username</label>
                  <input type="text" name="username" class="form-control" required>
                </div>

                <div class="form-group">
                  <label for="password2">Password</label>
                  <input type="password" name="password" class="form-control" required>
                </div>

                <input type="submit" value="Login" class="btn btn-secondary btn-block login__btn">
                <div class="login__notreg">
                  <span>Not registered?</span>
                  <a class="text-right" href="register.html">Register</a>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>	

<div class="login__footer">
<!-- @import "footer.kit" -->
</div>
</div>
<!-- @import "bootstrap.kit" -->
</body>
</html>